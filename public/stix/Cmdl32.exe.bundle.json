      "name": "Load of dll or ocx files from suspicious locations",
      "description": "Detects cmstp loading \"dll\" or \"ocx\" files from suspicious locations",
      "indicator_types": [
        "malicious-activity"
      ],
      "pattern": "[process:command_line MATCHES '^.*process\\.executable : \"\\*cmstp\\.exe\" and process\\.command_line : \\(\"\\*\\.dll\" or \"\\*\\.ocx\"\\).*$']",
